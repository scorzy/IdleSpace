    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@clr/angular": "^1.0.4",
    "@clr/icons": "^1.0.4",
    "@clr/ui": "^1.0.4",
    "@webcomponents/custom-elements": "^1.0.0",
    "ant-utils": "^1.2.0",
    "break_infinity.js": "^1.0.5",
