{
  "name": "idle-space",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run compileCompResult && ng serve --open",
    "start:prod": "npm run compileCompResult && ng serve --open --prod",
    "build": "npm run compileCompResult && ng build --prod --aot --build-optimizer --base-href ./",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint IdleSpace --fix",
    "e2e": "ng e2e",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "compileCompResult":"tsc  -p myTsconfig.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.3.2",
    "@angular/common": "~7.2.0",
